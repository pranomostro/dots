#global settings
defaults
tls on
#tls_starttls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt

#gmail account
account gmail
host smtp.gmail.com
#port 587
port 465
auth on
passwordeval spm show pranomestro@gmail
user pranomestro
from pranomestro@gmail.com
logfile /tmp/msmtp.log

#set the default account
account default: gmail
