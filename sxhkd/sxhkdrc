#very minimal configuration file for sxhkd
mod4 + Return
	st
mod4 + space
	dmenu
mod4 + q
	pkill Xorg

#wmutils related stuff

mod4 + {n, r, t, d}
	wmv {-10 0,0 +10, 0 -10, +10 0} $(pfw)
mod4 + {u, i, a, e}
	closest {west, south, north, east}
mod4 + {o, s}
	wsch {1.05, 0.95} $(pfw)
mod4 + k
	killw $(pfw)
mod4 + c
	cnt $(pfw)
mod4 + b
	beau $(pfw)
mod4 + w
	tile
mod4 + f
	fullscreen $(pfw)
